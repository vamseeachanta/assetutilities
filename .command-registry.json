  "commands": {
    "/modernize-deps": {
      "description": "Modernize dependency management and unify requirements into pyproject.toml",
      "source": ".agent-os/commands/modernize_deps.py",
      "type": "python",
      "category": "dependency-management",
      "requirements": ["python>=3.9", "uv"],
      "options": {
        "--target-dir": "Directory containing repositories (default: current)",
        "--parallel": "Number of parallel workers (default: 5)",
        "--no-backup": "Skip creating backups",
        "--repos": "Specific repositories to process",
        "--dry-run": "Show what would be done without making changes"
      },
      "examples": [
        "./slash_commands.py /modernize-deps",
        "./slash_commands.py /modernize-deps --parallel=10",
        "./slash_commands.py /modernize-deps --repos repo1 repo2",
        "./slash_commands.py /modernize-deps --dry-run"
      ]
    },
    "/propagate-commands": {
      "description": "Distribute slash commands from this repository to all others",
      "source": ".agent-os/commands/propagate_commands.py",
      "type": "python",
      "category": "command-management",
      "requirements": ["python>=3.9"],
      "options": {
        "--source": "Source repository (default: current directory)",
        "--target-dir": "Directory containing target repositories",
        "--commands": "Specific commands to propagate",
        "--parallel": "Number of parallel workers (default: 5)",
        "--force": "Force overwrite existing files",
        "--dry-run": "Show what would be done",
        "--repos": "Specific repositories to target"
      },
      "examples": [
        "./slash_commands.py /propagate-commands",
        "./slash_commands.py /propagate-commands --commands /modernize-deps",
        "./slash_commands.py /propagate-commands --target-dir /mnt/github/github",
        "./slash_commands.py /propagate-commands --force --parallel=10"
      ]
    },
